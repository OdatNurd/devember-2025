{
  "name": "obsidian-sample-plugin",
  "type": "module",
  "version": "1.0.0",
  "description": "This is a sample plugin for Obsidian (https://obsidian.md)",
  "author": "Terence Martin <odatnurd@gmail.com>",
  "license": "MIT",
  "keywords": ["obsidian"],
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.js",
    "build": "svelte-check && node esbuild.config.js production",
    "lint": "eslint .",
    "check": "svelte-check",
    "validate": "pnpm lint && pnpm check",
    "version": "node tools/version-bump.js"
  },
  "imports": {
    "#commands/*": "./src/commands/*.ts",
    "#components/*.svelte": "./src/components/*.svelte",
    "#components/*": "./src/components/*.ts",
    "#factory/*": "./src/factory/*.ts",
    "#modals/*": "./src/modals/*.ts",
    "#types": "./src/types.ts",
    "#plugin": "./src/plugin.ts",
    "#utils/*": "./src/utils/*.ts",
    "#views/*": "./src/views/*.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@typescript-eslint/parser": "^8.48.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "esbuild-svelte": "^0.9.3",
    "eslint": "^9.39.1",
    "eslint-plugin-svelte": "^3.13.0",
    "globals": "^16.5.0",
    "obsidian": "latest",
    "svelte": "^5.45.3",
    "svelte-check": "^4.3.4",
    "svelte-eslint-parser": "^1.4.0",
    "svelte-preprocess": "^6.0.3",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.48.0",
    "typescript-svelte-plugin": "^0.3.50"
  }
}
